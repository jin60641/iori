#search_wrap {
	width : 100vw;
	height : 120px;
}

#search_container {
	position : fixed;
	z-index : 100;
	width : 100vw;
	box-shadow : 0px 4px 8px rgba(0,0,0,.2);
	height : 120px;
	background-color : white;
}

#search_container_back {
	width : 100%;
	height : 70px;
	background-color : <%= color_hex %>;
	box-shadow : inset 0px 0px 1px 1px rgba(0,0,0,0.1);
	color : white;
}

#search_result_wrap {
	box-sizing : border-box;
	width : 650px;
	max-width : 100vw;
}


#container_query {
	font-size : 30px;
	text-align : left;
	padding-left : 14px;
	margin : auto;
	max-width : 100vw;
	width : 1300px;
	line-height : 65px;
	box-sizing : border-box;
}

#search_tab {
	padding-bottom : 0px;
	background-color : white;
	font-size: 13px;
	box-sizing : border-box;
	letter-spacing: 0px;
	max-width : 100vw;
	margin : auto;
	width : 1290px;
	border : 0;
	text-align : left;
}


#search_tab > div {
	cursor : pointer;
	display : inline-block;
	width : 70px;
	text-align : center;
	text-decoration : none;
	box-sizing : auto;
	box-sizing : initial;
	font-size : 17px;
	padding-top : 15px;
	height : 30px;
}

#search_tab > div:hover {
	color : <%= color_hex %>;
}

#search_tab > div::after {
	content: '';
	margin-top : 12px;
	display: block;
	width: 100%;
	height : 0px;
	background : <%= color_hex %>;
}

#search_tab > div:hover::after {
	margin-top : 7px;
	padding-bottom : 5px;
}

#search_result_none {
	margin : auto;
	box-sizing : border-box;
	padding : 19px 10px 20px 10px;

	width : 100%;

	background-color: white;

	border: 1px solid;
	border-color: #e5e6e9 #dfe0e4 #d0d1d5;
	border-radius: 3px;

	text-decoration: none;
	text-align : center;

	letter-spacing: 0px;
	font-size : 14px;
}

@media screen and ( max-width : 1300px ) {
	#search_wrap #search_container #search_tab, #container_query {
		width : 975px !important;
	}
}

@media screen and ( max-width : 650px ) {
	.user_list_div {
		display : block;
		margin-left : auto;
		margin-right : auto;
		margin-bottom : 20px;
	}
}

